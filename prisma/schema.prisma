
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}


model User {
  id       String @id @default(uuid())
  email    String
  password String
  name     String
  createdEmployee Employee[]
}
model Employee{
    id String @id @default(uuid())
    forstName String
    LastName String
    age String
    adress String
    user User @relation(fields:[userId],references:[id])
    userId String
}
